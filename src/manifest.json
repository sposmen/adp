{
  "name": "ADP Next",
  "content_scripts": [
    {
      "matches": [
        "*://workforcenow.adp.com/*"
      ],
      "css": [
        "app.css"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "vendor.js",
    "app.js"
  ],
  "externally_connectable": {
    "matches": [
      "*://workforcenow.adp.com/*"
    ]
  },
  "description": "Add some useful features to ADP",
  "version": "0.7.0",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "ADP Next",
    "default_icon": "assets/img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "assets/img/icon-48.png",
    "72": "assets/img/icon-72.png",
    "128": "assets/img/icon-128.png"
  },
  "manifest_version": 2
}